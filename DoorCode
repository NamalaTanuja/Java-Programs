package anudip;
import java.util.*;
public class DoorCode {

		public static void main(String[] args) {
			Scanner sc = new Scanner(System.in);
			int n = sc.nextInt();
			int firstDigit = n / 1000;
			int lastDigit = n % 10;
			int code = firstDigit + lastDigit;
			System.out.println(code);


		}

	

}

