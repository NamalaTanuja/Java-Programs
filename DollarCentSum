package javabean;

import java.util.*;

public class DollarCentSum {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int dollar1 = sc.nextInt();
		int cent1 = sc.nextInt();
		int dollar2 = sc .nextInt();
		int cent2 = sc.nextInt();
		int totalCents = cent1+cent2;
		int extraDollar = totalCents/100;
		int finalCent = totalCents %100;
		int totalDollar = dollar1 + dollar2+extraDollar;
		System.out.println(totalDollar);
		System.out.println(finalCent);
		
		

	}

}
