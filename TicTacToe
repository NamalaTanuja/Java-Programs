package javabean;

import java.util.*;

public class TicTacToe {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		if(n<1 || n>9)
		{
			System.out.println("invaild number");
		}
		int row = (n-1) /3;
		int col = (n-1) %3;
		System.out.println(row + " " +col);
	}

}
